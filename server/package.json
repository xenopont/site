{
    "name": "web-site-server",
    "version": "0.0.1",
    "description": "Website server application",
    "main": "app.ts",
    "scripts": {
        "lint": "node ./node_modules/tslint/bin/tslint --project .",
        "build": "node ./node_modules/parcel/bin/cli.js build ./src/index.html",
        "watch": "node ./node_modules/parcel/bin/cli.js watch ./src/index.html",
        "production": "node ./node_modules/parcel/bin/cli.js build ./src/index.html --no-source-maps"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/xenopont/site.git"
    },
    "author": "Sergei Kovalenko",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/xenopont/site/issues"
    },
    "homepage": "https://github.com/xenopont/site#readme",
    "dependencies": {
        "express": "4.17.1"
    },
    "devDependencies": {
        "tslint": "latest",
        "typescript": "latest",
        "nodemon": "latest",
        "@types/node": "latest"
    }
}
