{
    "name": "web-site-server",
    "version": "0.0.1",
    "description": "Website server application",
    "main": "app.ts",
    "scripts": {
        "build": "node ./node_modules/.bin/tsc",
        "watch": "node ./node_modules/.bin/nodemon",
        "lint": "node ./node_modules/.bin/eslint ./src/**.ts ./test/**.ts",
        "test": "node ./node_modules/.bin/mocha --reporter spec --recursive ./test/**/*.test.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/xenopont/site.git"
    },
    "author": "Sergei Kovalenko",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/xenopont/site/issues"
    },
    "homepage": "https://github.com/xenopont/site#readme",
    "dependencies": {
        "express": "4.17.1",
        "mongodb": "3.6.3"
    },
    "devDependencies": {
        "@types/express": "latest",
        "@types/mocha": "latest",
        "@types/mongodb": "latest",
        "@types/node": "latest",
        "@types/sinon": "latest",
        "@typescript-eslint/parser": "latest",
        "eslint": "latest",
        "mocha": "latest",
        "nodemon": "latest",
        "should": "latest",
        "sinon": "latest",
        "ts-node": "latest",
        "typescript": "latest"
    }
