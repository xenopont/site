{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "semicolon": false,
        "quotemark": [true, "single", "avoid-escape", "avoid-template"],
        "max-line-length": [true, 128],
        "one-line": [false, "check-catch", "check-finally", "check-else"]
    },
    "rulesDirectory": []
}